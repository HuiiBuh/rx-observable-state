{
  "name": "rx-observable-state",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "build:w": "tsc -p tsconfig.json -w",
    "test": "jest",
    "format": "prettier --write .",
    "coverage": "jest --collect-coverage",
    "lint": "tslint -p tsconfig.json",
    "minifyJson": "jq -c . < assets/json1.json > assets/json1.tmp.json && jq -c . < assets/json2.json > assets/json2.tmp.json && mv assets/json1.tmp.json assets/json1.json && mv assets/json2.tmp.json assets/json2.json"
  },
  "peerDependencies": {
    "rxjs": ">6"
  },
  "keywords": [
    "angular",
    "rxjs",
    "immutable",
    "state",
    "state management",
    "store"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.4",
    "jest": "^27.4.5",
    "prettier": "2.5.1",
    "rxjs": "^7.4.0",
    "ts-jest": "^27.1.2",
    "ts-toolbelt": "^9.6.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  }
}
